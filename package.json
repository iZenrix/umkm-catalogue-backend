{
  "name": "umkm-catalogue-backend",
  "version": "1.0.0",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "build:prod": "pnpm prisma:generate && pnpm build",
    "postinstall": "prisma generate",

    "env:check": "cross-env NODE_ENV=production ts-node scripts/checkEnv.ts",

    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:dev:reset": "prisma migrate reset",

    "migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
    "migrate:prod:status": "dotenv -e .env.prod -- prisma migrate status",
    "migrate:prod:reset": "dotenv -e .env.prod -- prisma migrate reset",

    "db:seed:dev": "prisma db seed",
    "db:seed:prod": "dotenv -e .env.prod -- prisma db seed",

    "prisma:studio:prod": "dotenv -e .env.prod -- prisma studio",
    "prisma:generate:prod": "dotenv -e .env.prod -- prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.8.7",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@vercel/blob": "^0.26.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1"
  }
}
